{
    "name": "mu-capstone-server",
    "version": "0.0.1",
    "type": "module",
    "scripts": {
        "mac:mongod-start": "mongod --config \"$(brew --prefix)/etc/mongod.conf\" &",
        "mac:mongod-kill": "pkill mongod",
        "mac:mongo-reset": "echo 'db.UserbaseC.deleteMany({})' | mongosh --quiet bunkiez && mongoimport --jsonArray -d bunkiez -c UserbaseC users.json",
        "build": "npx tsc",
        "dev": "nodemon --watch 'src/**/*.ts' src/index.ts",
        "test": "jest"
    },
    "dependencies": {
        "@types/express": "^4.17.14",
        "bcrypt": "^5.1.0",
        "chance": "^1.1.9",
        "cors": "^2.8.5",
        "dotenv": "^16.0.3",
        "express": "^4.18.2",
        "express-jwt": "^7.7.7",
        "express-validator": "^6.14.2",
        "jsonwebtoken": "^8.5.1",
        "mongoose": "^6.7.0",
        "socket.io": "^4.5.3"
    },
    "devDependencies": {
        "@tsconfig/node18": "^1.0.1",
        "@types/bcrypt": "^5.0.0",
        "@types/chance": "^1.1.3",
        "@types/jest": "^29.2.2",
        "@types/jsonwebtoken": "^8.5.9",
        "@types/supertest": "^2.0.12",
        "concurrently": "^7.5.0",
        "jest": "^29.3.0",
        "nodemon": "^2.0.20",
        "supertest": "^6.3.1",
        "ts-jest": "^29.0.3",
        "ts-node": "^10.9.1",
        "typescript": "^4.8.4"
    }
}
